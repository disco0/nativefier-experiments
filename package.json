  "scripts": {
    "build": "ts-node --project ./src/tsconfig.json --prefer-ts-exts --pretty ./src",
    "build:all:tsconfig": "yarn clean && ts-node --pretty --prefer-ts-exts --project ./src/tsconfig.json ./src",
    "build:all": "ts-node --files ./src -a",
    "build:current:tsconfig": "ts-node --project ./src/tsconfig.json ./src",
    "build:current": "ts-node --files ./src ",
    "build:ts-node": "ts-node --project ./src/tsconfig.json --prefer-ts-exts --pretty ./src ",
    "clean": "rm -rf ./build",
    "build:clean": "yarn clean && yarn build:all",
    "webpack:ts:env": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\"",
    "build:regex101:injects:webpack": "cd src/injects/regex101 && tsc ./webpack.config.ts && webpack --config ./webpack.config.js --mode development",
    "build:regex101": "yarn clean && yarn build:regex101:injects:webpack && yarn build --schemas regex --max 1 && yarn open:regex101:darwin",
    "open:regex101:darwin": "open build/Regex101-darwin-x64/Regex101.app",
    "injects:regex101:enter": "pushd src/injects/regex101",
    "injects:regex101:build": "yarn injects:regex101:enter && "
  },
